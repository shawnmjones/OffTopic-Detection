    function mostreadtab(current, aname) {


        if (current == "") {
            current = 0;
        }
        var myItems = new Array(1, 2, 3);
        for (var i = 0, len = myItems.length; i < len; ++i) {
            currentitem = myItems[i];

            if (current == currentitem) { // this is active tab
                SetActiveTab(currentitem, aname);
            } else {
                InActiveTab(currentitem, aname);
            }
        }

    }


    function InActiveTab(current, aname) {

        var LItabs = document.getElementById(aname + current);        
        if (IE == 0) {
            LItabs.setAttribute("class", 'normal_button_p');
        } else {
            LItabs.setAttribute('className', 'normal_button_p');
        }        
        return;
    }

    function SetActiveTab(current, aname) {

        var LItabs = document.getElementById(aname + current);        
        if (IE == 0) {
                LItabs.setAttribute("class", 'active_button_p');
        } else {
                LItabs.setAttribute('className', 'active_button_p');
        }        
        return;

    }
    
</script>
