                var MainNewsIds = new Array('0','164978','164978','164978','164978','164978','164978');
                var MainNewsImages = new Array('0','/MediaFiles/2012/8/10/2012-08-09t143834z_10_8_2012_48_50.jpg','/MediaFiles/2012/8/10/HJ_10_8_2012_18_14.jpg','/MediaFiles/2012/8/10/erter_9_8_2012_45_20.jpg','/MediaFiles/2012/8/10/b---ilil_10_8_2012_22_50.jpg','/MediaFiles/2012/8/10/b-----uyyym_10_8_2012_28_55.jpg','/MediaFiles/2012/8/10/f2_9_8_2012_51_24.jpg');
                var MainNewsTitles = new Array('0' ,'الحملة العسكرية بسيناء تسفر عن ضبط 6 من المشتبه بتورطهم فى أحداث رفح' ,'الخبير الاستراتيجي اللواء‏.‏ د‏.‏ محمود خلف‏:‏الشبهات تحوم حول إيران‏..‏ وإسرائيل تعمل بـمبدأ نابليون' ,'تدمــير‏150‏ مــن أنفـاق رفـح <br>شيوخ القبائل‏ :5000 متطرف يختبئون في الجبال' ,'الأسد يعين رئيسا للوزراء خلفا لحجاب‏<br>وانشقاق مدير المراسم و‏100‏ ضابط' ,'صحيفة اسرائيلية‏:‏ السعودية هددت باستهداف<br>أي طائرة إسرائيلية تعبر أجوائها باتجاه إيران' ,'ســــيناء‏ ..‏ فريســـــة التــطرف والإهـمال<br>أحد حائزى الاسلحة المهربة من ليبيا واسرائيل : لدينا كميات من السلاح تكفى للتصدى لجيش بكامله');
                var MainNewsAbstract = new Array('0','تواصلت الحملة العسكرية والامنية بمناطق جنوب العريش والشيخ زويد ورفح صباح اليوم "الجمعة"، وأسفرت عن ضبط عدد 6 من المشتبه بتورطهم في أحداث الإعتداء على نقاط الحدود برفح','في رفح لم يكتف الإرهاب القبيح بنهش زهرة شبابنا الأبرار‏,‏ بل أسقط ورقة التوت عن واقع مفزع تعيشه مصر‏,‏ وهي تخشي الانزلاق إلي الفوضي‏,‏ شيء لا يصدقه عقل‏.','وصلت أعداد كبيرة من المدرعات والقاذفات الصاروخية والمجنزرات إلي مدينة العريش أمس لتعزيز القوات المنتشرة في شمال سيناء‏,‏ بينما بدأت معدات  تابعة لسلاح المهندسين في هدم عدد كبير يقدر بنحو150 من أنفاق التهريب الممتدة من رفح إلي داخل قطاع غزة.',' عين الرئيس السوري بشار الاسد وزير الصحة وائل الحلقي رئيسا للوزراء أمس بعد انشقاق رئيس الوزراء السابق رياض حجاب الأسبوع الماضي‏,في وقت تستضيف فيه إيران مؤتمرا لوزراء الخارجية سعيا لانهاء الصراع .',' أكدت صحيفة يديعوت أحرونوت الإسرائيلية أن الرياض أبلغت الجانب الإسرائيلي‏-‏ عبر مسئولين أمريكيين‏-‏ أنها ستعترض أي طائرة إسرائيلية تعبر مجالها الجوي في طريقها إلي إيران‏.‏','عابس وجه سيناء مكفهرة رمالها حزين هواء الصيف في فضائها الرحب عندما وصلت إلي مشارف الحدود جثوت علي ركبتي وأطلب البكاء والتأمل لأنني وجدتها وكأنها غريبة علي وأنا غريب فيها أو أن كلينا بدأ ارتحال الغربة والتغريب‏');
                var MainNewsLinks = new Array('0','Al-Mashhad-Al-Syiassy/News/164978.aspx','Al-Ahram-Files/News/164821.aspx','The-First/News/164829.aspx','Arab-world/News/164909.aspx','World/News/164845.aspx','Al-Ahram-Files/News/164823.aspx');
                var MainNewsCommentsNumber = new Array('0','6','55','35','0','13','1');
