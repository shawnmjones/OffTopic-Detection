                var MainNewsIds = new Array('0','116559','116559','116559','116559','116559','116559');
                var MainNewsImages = new Array('0','/MediaFiles/2011/12/4/b---hkl;_3_12_2011_26_23.jpg','/MediaFiles/2011/12/4/main1_3_12_2011_56_33.jpg','/MediaFiles/2011/12/4/1111111thghj_4_12_2011_2_50.jpg','/MediaFiles/2011/12/4/111111111111111111111111_4_12_2011_10_57.jpg','/MediaFiles/2011/12/4/a1b1_3_12_2011_55_54.jpg','/MediaFiles/2011/12/4/1a_3_12_2011_27_21.jpg');
                var MainNewsTitles = new Array('0' ,'مواجهة شرسة علي المقاعد الفردية في انتخابات الإعادة غدا...نتائج القوائم‏:‏ الحرية والعدالة‏3.6‏ مليون صوت والنور‏2.4‏ و الكتلة المصرية‏1.3‏ الوفد‏700‏ ألف' ,'في الاسكندرية:الإخوان والسلفيون يحصدون أعلي النسب في القوائم الحزبية :6مقاعد للحرية والعدالة و 5 للنور و2 للكتلة و2 للوفد ومقعد للثورة' ,'عملية قيصرية لحكومة الإنقاذ <br>الجنزوري:اعادة النظر في عدد من المرشحين' ,'بهدفين في مرمي جنوب إفريقيا...المنتخب الأوليمبي<br>يتأهل إلي قبل نهائي التصفيات الأوليمبية' ,'اللجنة الوزارية العربية تمنح سوريا مهلة إضافية<br>تنتهي اليوم للموافقة علي المبادرة العربية<br>' ,'واشنطن ترفض القفز علي نتائج الانتخابات المصرية<br>وتؤكد استعدادها للتحاور مع أي حكومة مقبلة');
                var MainNewsAbstract = new Array('0','وسط مواجهة شرسة علي المقاعد الفردية‏,‏ يتنافس‏104‏ مرشحين غدا علي‏52‏ مقعدا في‏27‏ دائرة خلال جولة الإعادة في انتخابات المرحلة الأولي لمجلس الشعب‏,‏ بمحافظات‏:‏ القاهرة والإسكندرية وأسيوط والأقصر وكفر الشيخ وبورسعيد والبحر الأحمر ودمياط والفيوم‏.‏','اسفرت نتائج فرز القوائم بالدائرتين الأولي والثانية بالإسكندرية عن فوز حزب الحرية والعدالة ب6 مقاعد باجمالي عدد اصوات ‏754‏ ألفا و‏600‏ صوت وحزب النور ب5 مقاعد بأصوات‏678‏ ألفا و‏51‏ صوتا والكتلة المصرية بمقعدين باجمالي اصوات‏246‏ زلفا و‏712‏ صوتا‏.','أكد الدكتور كمال الجنزوري رئيس مجلس الوزراء‏,‏ قبيل توجهه للقاء المجلس العسكري أمس‏,‏ أنه لم ينته بعد من الاستقرار علي الشكل النهائي للوزارة الجديدة وأنه سيعيد النظر في أسماء المرشحين الذين قابلهم علي مدي الايام الماضية لتولي حقائب وزارية .‏','حقق المنتخب الأوليمبي فوزا كبيرا علي نظيره جنوب افريقيا بهدفين لمحمد صلاح ومروان محسن تم احرازهما في الشوط الثاني للمباراة ليتأهل المنتخب إلي الدور قبل النهائي كأول للمجموعة بعد أن صنع منتخب الجابون مفاجأة من العيار الثقيل بالفوز علي كوت ديفوار 3-1 .','قررت اللجنة الوزارية العربية المعنية بالوضع في سوريا منح دمشق مهلة جديدة تنتهي اليوم للموافقة المبادرة العربية التي تهدف إلي وقف العنف ضد المحتجين صرح بذلك الشيخ حمد بن جاسم  رئيس الوزراء القطري رئيس اللجنة في ختام اجتماعها مساء امس بالدوحة‏.‏','‏ واصلت الانتخابات البرلمانية المصرية فرض هيمنتها علي تصريحات المسئولين الغربيين‏,‏ فاتفق السياسيون علي جانبي الأطلنطي علي وصف الانتخابات بأنها خطوة إيجابية ومشجعة كبداية لعملية التحول الديمقراطي في مصر‏.');
                var MainNewsLinks = new Array('0','The-First/News/116559.aspx','Revolution-Parliament/News/116586.aspx','Egypt/News/116653.aspx','Sport/News/116568.aspx','Arab-world/News/116593.aspx','World/News/116554.aspx');
