<script type="text/javascript">

    function GetWriters_New(wType,divname,aname,current,issueID) {
        jx.load("AhramWriterTopics.aspx?IssueID=" + issueID + "&wtype=" + wType , function(data) {
        document.getElementById(divname).innerHTML = data        
        });
        mostreadtab(current, aname);
